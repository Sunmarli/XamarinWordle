<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XamarinWordle.MainPage">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Start Game" Clicked="StartGameButton_Clicked" />
            <Button Text="Modify Words" Clicked="ModifyWordsButton_Clicked" />
            <Button Text="Add New Word" Clicked="AddWordButton_Clicked" />
        </StackLayout>

    </ContentPage.Content>
</ContentPage>